<!-- Page body start -->
<div class="page-body" ng-init="getPreApplicationConfigById()">
    <div class="row">
        <div class="col-sm-12">
            <!-- Basic Form Inputs card start -->
            <div class="card">
                <div class="card-header">
                    <div>
                        <div class="float-left">
                            <h4>Add Pre-Application Configuration</h4>
                        </div>
                        <div class="float-right">
                            <button type="button" ng-click="backToListAppConfig()" class="tabledit-edit-button btn btn-primary waves-effect waves-light ">Modify</button>&nbsp;&nbsp;
                        </div>
                    </div>
                </div>

                <div class="card-block">
                    <nav class="navbar navbar-expand-lg navbar-light light-nav-border">
                        <span style="font-size:medium;color:navy"><b>{{ProgrammeInstanceName}} / {{PartName}} / {{ProgrammePartTermName}} </b></span>
                    </nav>
                    <br />

                    <div class="col-md-12">
                        <form ng-model="AppConfig">

