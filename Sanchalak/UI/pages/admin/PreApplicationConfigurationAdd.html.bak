                            <div class="form-group row">
                                <label class="col-sm-3">Application Start Date</label>
                                <div class="col-sm-3">
                                    <input id="appstart" name="appstart" type="date" class="form-control" ng-model="AppConfig.ApplicationStartDate" />
                                </div>

                                <label class="col-sm-3">Application End Date</label>
                                <div class="col-sm-3">
                                    <input id="append" name="append" type="date" class="form-control" ng-model="AppConfig.ApplicationStopDate" />
                                </div>

                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3">ApplicationFee Start Date</label>
                                <div class="col-sm-3">
                                    <input id="appfeestart" name="appfeestart" type="date" class="form-control" ng-model="AppConfig.ApplicationFeeStartDate" />
                                </div>

                                <label class="col-sm-3">ApplicationFee End Date</label>
                                <div class="col-sm-3">
                                    <input id="appfeeend" name="appfeeend" type="date" class="form-control" ng-model="AppConfig.ApplicationFeeEndDate" />
                                </div>

                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3">AdmissionFees Start Date</label>
                                <div class="col-sm-3">
                                    <input id="admfeestart" name="admfeestart" type="date" class="form-control" ng-model="AppConfig.AdmissionFeesStartDate" />
                                </div>

                                <label class="col-sm-3">AdmissionFees End Date</label>
                                <div class="col-sm-3">
                                    <input id="admfeeend" name="admfeeend" type="date" class="form-control" ng-model="AppConfig.AdmissionFeesStopDate" />
                                </div>

                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3">Application Approval Start Date</label>
                                <div class="col-sm-3">
                                    <input id="approvalstart" name="approvalstart" type="date" class="form-control" ng-model="AppConfig.ApplicationApprovalStartDate" />
                                </div>

                                <label class="col-sm-3">Application Approval End Date</label>
                                <div class="col-sm-3">
                                    <input id="approvalend" name="approvalend" type="date" class="form-control" ng-model="AppConfig.ApplicationApprovalStopDate" />
                                </div>

                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3">Last Date of Uploading Pending Documents</label>
                                <div class="col-sm-3">
                                    <input id="docend" name="docend" type="date" class="form-control" ng-model="AppConfig.UpdatedDocLastDate" />
                                </div>

                                <label class="col-sm-3">Remarks<span style="color:red;"> *</span></label>
                                <div class="col-sm-3">
                                    <input id="remarks" name="remarks" type="text" class="form-control" ng-model="AppConfig.Remarks" required />
                                </div>

                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3">Prospectus URL<span style="color:red;"> *</span></label>
                                <div class="col-sm-3">
                                    <input id="prospectusURL" name="prospectusURL" type="text" class="form-control" ng-model="AppConfig.ProspectusURL" required />
                                </div>

                                <label class="col-sm-3">Written Test Syllabus URL</label>
                                <div class="col-sm-3">
                                    <input id="writtenTestSyllabusURL" name="writtenTestSyllabusURL" type="text" class="form-control" ng-model="AppConfig.WrittenTestSyllabusURL" />
                                </div>

                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3">Contact Person Name</label>
                                <div class="col-sm-3">
                                    <input id="ContactPersonName" name="ContactPersonName" type="text" class="form-control" maxlength="10" ng-model="AppConfig.ContactPersonName" />
                                </div>

                                <label class="col-sm-3">Contact Person Number</label>
                                <div class="col-sm-3">
                                    <input id="ContactPersonNo" name="ContactPersonNo" type="number" class="form-control" maxlength="11" ng-model="AppConfig.ContactPersonNo" />
                                </div>

                            </div>

                            <div class="form-group row"  ng-init="getApplicationFee()">
                                <label class="col-sm-3">Contact Person Email</label>
                                <div class="col-sm-3">
                                    <input id="ContactPersonEmail" name="ContactPersonEmail" type="text" class="form-control" ng-model="AppConfig.ContactPersonEmail" />
                                </div>

                                <label class="col-sm-3">Select Application Fee Category</label>
                                <div class="col-sm-3">
                                    <div>
                                        <span class="nullable">
                                            <select ng-model="FEECONFIG.ApplicationFeeId" ng-options="d.Id as d.FeeCategoryName for d in ApplicationFee" [value]="FEECONFIG.ApplicationFeeId" ng-change="ShowLableFeeDetail(FEECONFIG.ApplicationFeeId)" class="form-control" name="Progdropdown" required>
                                                <option value="" disabled selected>-- SELECT --</option>
                                            </select>
                                        </span>
                                    </div>
                                </div>

                            </div>

                           
                            <div ng-show="ShowFee">
                                <label class="col-form-label">Application Fee Category : {{Feedetail.FeeCategoryName}}</label><br />
                                <label class="col-form-label">Application Fee Amount : {{Feedetail.Amount}}</label>
                            </div>
                            <div class="col-md-12 offset-md-2">
                                <div class="form-group row">
                                    <div class="col-sm-6">
                                        <button class="btn btn-success" ng-click="addPreApplicationConfig()">Set Date</button>
                                        <button class="btn btn-disabled disabled" ng-click="resetAppConfig()">Cancel</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="card">
                        <div class="card-block">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered" id="example-2" ng-table="ApplicationConfigTableparam" show-filter="true">
                                    <tbody>
                                        <tr ng-repeat="AppConfig in AppConfigList">
                                            <td data-title="'Sr.No'" filter="{$index: 'text'}" sortable="'$index'" style="border-top:none;"><label>{{ $index + 1 }}</label></td>
                                            <td data-title="'FacultyName'" filter="{FacultyName: 'text'}" sortable="'FacultyName'" style="border-top:none;"><label>{{AppConfig.FacultyName}}</label></td>
                                            <td data-title="'ProgrammeName'" filter="{ProgrammeName: 'text'}" sortable="'ProgrammeName'" style="border-top:none;"><label>{{AppConfig.ProgrammeName}}</label></td>
                                            <td data-title="'PartTermName'" filter="{PartTermName: 'text'}" sortable="'PartTermName'" style="border-top:none;"><label>{{AppConfig.PartTermName}}</label></td>
                                            <td data-title="'ApplicationStartDate'" filter="{ApplicationStartDate: 'text'}" sortable="'ApplicationStartDate'" style="border-top:none;"><label>{{AppConfig.ApplicationStartDateView}}</label></td>
                                            <td data-title="'ApplicationStopDate'" filter="{ApplicationStopDate: 'text'}" sortable="'ApplicationStopDate'" style="border-top:none;"><label>{{AppConfig.ApplicationStopDateView}}</label></td>
                                            <td data-title="'ApplicationFeeStartDate'" filter="{ApplicationFeeStartDate: 'text'}" sortable="'ApplicationFeeStartDate'" style="border-top:none;"><label>{{AppConfig.ApplicationFeeStartDateView}}</label></td>
                                            <td data-title="'ApplicationFeeEndDate'" filter="{ApplicationFeeEndDate: 'text'}" sortable="'ApplicationFeeEndDate'" style="border-top:none;"><label>{{AppConfig.ApplicationFeeEndDateView}}</label></td>
                                            <td data-title="'AdmissionFeesStartDate'" filter="{AdmissionFeesStartDate: 'text'}" sortable="'AdmissionFeesStartDate'" style="border-top:none;"><label>{{AppConfig.AdmissionFeesStartDateView}}</label></td>
                                            <td data-title="'AdmissionFeesStopDate'" filter="{AdmissionFeesStopDate: 'text'}" sortable="'AdmissionFeesStopDate'" style="border-top:none;"><label>{{AppConfig.AdmissionFeesStopDateView}}</label></td>
                                            <td data-title="'ApplicationApprovalStartDate'" filter="{ApplicationApprovalStartDate: 'text'}" sortable="'ApplicationApprovalStartDate'" style="border-top:none;"><label>{{AppConfig.ApplicationApprovalStartDateView}}</label></td>
                                            <td data-title="'ApplicationApprovalStopDate'" filter="{ApplicationApprovalStopDate: 'text'}" sortable="'ApplicationApprovalStopDate'" style="border-top:none;"><label>{{AppConfig.ApplicationApprovalStopDateView}}</label></td>
                                            <td data-title="'UpdatedDocLastDate'" filter="{UpdatedDocLastDate: 'text'}" sortable="'UpdatedDocLastDate'" style="border-top:none;"><label>{{AppConfig.UpdatedDocLastDateView}}</label></td>
                                            <td data-title="'Remarks'" filter="{Remarks: 'text'}" sortable="'Remarks'" style="border-top:none;"><label>{{AppConfig.Remarks}}</label></td>
                                            <td data-title="'Prospectus URL'" filter="{ProspectusURL: 'text'}" sortable="'ProspectusURL'" style="border-top:none;"><label>{{AppConfig.ProspectusURL}}</label></td>
                                            <td data-title="'Written Test Syllabus URL'" filter="{WrittenTestSyllabusURL: 'text'}" sortable="'WrittenTestSyllabusURL'" style="border-top:none;"><label>{{AppConfig.WrittenTestSyllabusURL}}</label></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
