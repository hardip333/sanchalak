<!-- Page body start -->
<div class="page-body">
    <div class="row">
        <div class="col-sm-12">
            <!-- Basic Form Inputs card start -->
            <div class="card">
                <div class="card-header">
                    <h4><b>Search Student By PRN / Application Number To view Admission Details:</b> </h4>
                </div>
                <div class="card-block">
                    <div class="form-group row d-flex" ng-init="GetLocalStorageValue()">
                        <label class="col-sm-4 col-form-label"><bold>PRN / Applicant UserName / Application No</bold> <span style="color:red">*</span>:</label>
                        <div class="col-sm-5">
                            <input type="text" class="form-control" ng-model="AdmissionInfo.UserName" placeholder="Type UserName / PRN / Application Id">

                        </div>
                        <div class="col-sm-1">
                            <button class="btn btn-success" data-toggle="tooltip" data-placement="top" title="Click The Submit Button For Proceed" ng-click="SubmitRegistrationId()">Next </button>
                        </div>
                        <div class="col-sm-1 ml-auto" ng-show="AutoFetchedPRN">
                            <div data-toggle="tooltip" data-placement="bottom" title="Go To Previous Page" id="top-back-icon" ng-click="goBack()">
                                <i class="icofont icofont-circled-left"></i>
                            </div>
                        </div>

                    </div>


                    <div class="border border-primary" ng-show="IsTabVisible">
                        <ul class="nav nav-tabs md-tabs tab-timeline" role="tablist" id="mytab">
                            <li class="nav-item">
                                <a class="nav-link active" href="" data-toggle="tab" ng-click="TabP()" role="tab">
                                    <i class="icofont icofont-ui-user"></i>&nbsp;Personal Information Details

                                </a>

                                <div class="slide"></div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="" data-toggle="tab" ng-click="TabEdu()" role="tab">
                                    <i class="ion-briefcase"></i>&nbsp;Education Details
                                </a>
                                <div class="slide"></div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="" data-toggle="tab" ng-click="TabAd()" role="tab">
                                    <i class="ion-university"></i>&nbsp;Admission Details
                                </a>
                                <div class="slide"></div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="" data-toggle="tab" ng-click="TabPaper()" role="tab">

                                    <i class="ion-social-buffer"></i>&nbsp;Paper List Details
                                </a>
                                <div class="slide"></div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="" data-toggle="tab" ng-click="TabReq()" role="tab">

                                    <i class="ion-clipboard"></i>&nbsp;Request Status Details
                                </a>
                                <div class="slide"></div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="" data-toggle="tab" ng-click="TabPre()" role="tab">
                                    <i class="ion-aperture"></i>&nbsp;Pre Examination List Details
                                </a>
                                <div class="slide"></div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="" data-toggle="tab" ng-click="TabCancel()" role="tab">
                                    <i class="ion-close-circled"></i>&nbsp;Cancel Admission Details
                                </a>
                                <div class="slide"></div>
                            </li>
                        </ul>
                    </div>
                    <div class="col-lg-12 col-xl-12 border" ng-repeat="info in ApplicantAdmDetail.PersonalList" ng-show="PersonalList">
                        <div class="card-header">

                            <div class="text-center">
                                <h4 class="btn btn-info"> Personal Information Details</h4>
                            </div>


                        </div>


                        <div class="form-group row">
                            <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>User Name / PRN :</b> </label>
                            <label for="Activity" class="col-sm-2 col-form-label">{{info.UserName}}</label><br />

                            <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Name As Per Marksheet :</b> </label>
                            <label for="Activity" class="col-sm-4 col-form-label">{{info.NameAsPerMarksheet}}</label>

                        </div>

                        <div class="form-group row">
                            <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Mobile No :</b> </label>
                            <label for="Activity" class="col-sm-2 col-form-label">{{info.MobileNo}}</label>
                            <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Email Id :</b> </label>
                            <label for="Activity" class="col-sm-4 col-form-label">{{info.EmailId}}</label><br />


                        </div>

                    </div>

                    <div class="tab-content border" ng-show="Personal">
                        <div class="tab-pane active" role="tabpanel">
                            <div class="card-header">

                                <div class="text-center">
                                    <h4 class="btn btn-info"> Personal Information Details:</h4>
                                </div>


                            </div>
                        </div>

                        <div class="col-lg-12 col-xl-12" ng-repeat="info in ApplicantAdmDetail.PersonalList" ng-show="ApplicantAdmDetail.PersonalListflag==true">
                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>User Name / PRN :</b> </label>
                                <label for="Activity" class="col-sm-2 col-form-label">{{info.UserName}}</label><br />

                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Name As Per Marksheet :</b> </label>
                                <label for="Activity" class="col-sm-4 col-form-label">{{info.NameAsPerMarksheet}}</label>

                            </div>

                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Mobile No :</b> </label>
                                <label for="Activity" class="col-sm-2 col-form-label">{{info.MobileNo}}</label>
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Email Id :</b> </label>
                                <label for="Activity" class="col-sm-4 col-form-label">{{info.EmailId}}</label><br />


                            </div>

                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>First Name :</b> </label>
                                <label for="Activity" class="col-sm-2 col-form-label">{{info.FirstName}}</label><br />
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Last Name :</b> </label>
                                <label for="Activity" class="col-sm-4 col-form-label">{{info.LastName}}</label>

                            </div>
                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Middle Name :</b> </label>
                                <label for="Activity" class="col-sm-2 col-form-label">{{info.MiddleName}}</label><br />
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Gender :</b> </label>
                                <label for="Activity" class="col-sm-4 col-form-label">{{info.Gender}}</label>
                            </div>


                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Religion:</b> </label>
                                <label for="Activity" class="col-sm-2 col-form-label">{{info.Religion}}</label><br />
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Mother Tongue :</b> </label>
                                <label for="Activity" class="col-sm-4 col-form-label">{{info.MotherTongue}}</label>
                            </div>
                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Applicant Photo :</b> </label>

                                <label for="Activity" class="col-sm-2 col-form-label" ng-show="info.ApplicantPhoto  != null">

                                    <a href="{{info.ApplicantPhoto}}" target="_blank">
                                        <button type="button" title="View"
                                                class="btn btn-info waves-effect waves-light" style="min-width:40px;">
                                            <i class="icon-doc" style="color:white"></i>

                                        </button>
                                    </a>
                                </label>
                                <label for="Activity" class="col-sm-2 col-form-label" ng-show="info.ApplicantPhoto == null">
                                    <span style="color:red;">No Document Found..!</span>
                                </label>
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Applicant Signature :</b> </label>

                                <label for="Activity" class="col-sm-4 col-form-label" ng-show="info.ApplicantSignature  != null">

                                    <a href="{{info.ApplicantSignature}}" target="_blank">
                                        <button type="button" title="View"
                                                class="btn btn-info waves-effect waves-light" style="min-width:40px;">
                                            <i class="icon-doc" style="color:white"></i>

                                        </button>
                                    </a>
                                </label>
                                <label for="Activity" class="col-sm-4 col-form-label" ng-show="info.ApplicantSignature == null">
                                    <span style="color:red;">No Document Found..!</span>
                                </label>

                            </div>

                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Is ApplicantSignature Verified :</b> </label>
                                <label ng-show="info.IsApplicantSignatureVerified" for="Activity" class="col-sm-2 col-form-label">Yes</label>
                                <label ng-hide="info.IsApplicantSignatureVerified" for="Activity" class="col-sm-2 col-form-label">No</label>
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Is ApplicantPhoto Verified :</b> </label>
                                <label ng-show="info.IsApplicantPhotoVerified" for="Activity" class="col-sm-4 col-form-label">Yes</label>
                                <label ng-hide="info.IsApplicantPhotoVerified" for="Activity" class="col-sm-4 col-form-label">No</label>
                            </div>

                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Is PhotoId Doc Verified :</b> </label>
                                <label ng-show="info.IsPhotoIdDocVerified" for="Activity" class="col-sm-2 col-form-label">Yes</label>
                                <label ng-hide="info.IsPhotoIdDocVerified" for="Activity" class="col-sm-2 col-form-label">No</label>
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Is Confirm :</b> </label>
                                <label ng-show="info.IsConfirm" for="Activity" class="col-sm-4 col-form-label">Yes</label>
                                <label ng-hide="info.IsConfirm" for="Activity" class="col-sm-4 col-form-label">No</label>
                            </div>


                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Email Id :</b> </label>
                                <label for="Activity" class="col-sm-2 col-form-label">{{info.EmailId}}</label><br />
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Mobile No :</b> </label>
                                <label for="Activity" class="col-sm-4 col-form-label">{{info.MobileNo}}</label>

                            </div>
                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Name Of Father:</b> </label>
                                <label for="Activity" class="col-sm-2 col-form-label">{{info.NameOfFather}}</label><br />
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Name Of Mother :</b> </label>
                                <label for="Activity" class="col-sm-4 col-form-label">{{info.NameOfMother}}</label>
                            </div>
                            <div class="form-group row">
                                <label for="Activity" class="col-sm-3 col-form-label">&nbsp;&nbsp;&nbsp;&nbsp;<b>Occupation Of Father:</b> </label>
                                <label for="Activity" class="col-sm-2 col-form-label">{{info.OccupationOfFather}}</label><br />
