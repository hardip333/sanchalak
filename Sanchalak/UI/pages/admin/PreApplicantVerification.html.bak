
<script type="text/javascript">

    var currentYear = new Date().getFullYear();

    for (y = 1950; y <= currentYear; y++) {
        var optn = document.createElement("OPTION");
        optn.text = y;
        optn.value = y;


        document.getElementById('year').options.add(optn);
    }
</script>

